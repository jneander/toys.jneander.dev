{
  "name": "cs.jneander.dev",
  "scripts": {
    "build": "npm-run-all clean build:prod:*",
    "build:dev": "npm-run-all clean build:dev:*",
    "build:dev:webpack": "webpack build --config ./config/webpack.development.js",
    "build:prod:11ty": "eleventy --quiet",
    "build:prod:webpack": "webpack build --config ./config/webpack.production.js",
    "clean": "rimraf ./dist",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "start": "npm-run-all clean build:dev -p start:*",
    "start:11ty": "eleventy --serve --quiet --watch",
    "start:webpack": "webpack --watch --config ./config/webpack.development.js",
    "test:types": "tsc --noEmit",
    "test:types:watch": "tsc --noEmit --watch"
  },
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@jneander/genetics": "^0.7.0",
    "@jneander/utils-async": "^2.1.1",
    "@jneander/utils-random": "^1.0.0",
    "@jneander/utils-state": "^2.0.2",
    "chart.js": "^3.7.0",
    "p5": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.4",
    "@jneander/prettier-config": "^1.0.0",
    "@types/node": "^18.0.0",
    "@types/p5": "^1.3.3",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.3.0",
    "eleventy-sass": "^2.2.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
