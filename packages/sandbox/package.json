{
  "name": "sandbox",
  "engines": {
    "node": ">=6.9.5"
  },
  "version": "0.0.0",
  "description": "A place to exercise various projects together and dogfood their development environments",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.development.js --progress",
    "build:production": "webpack --config ./config/webpack.production.js --progress",
    "build:stats": "webpack --config ./config/webpack.development.js --profile --json",
    "build:watch": "webpack --config ./config/webpack.development.js --progress --watch",
    "clean": "rimraf __build__",
    "deploy": "node ./scripts/deploy.js",
    "spec": "karma start --single-run --no-auto-watch config/karma.conf.js",
    "spec:watch": "karma start config/karma.conf.js",
    "start:watch": "node scripts/dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jneander/sandbox.git"
  },
  "author": "Jeremy Neander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jneander/sandbox/issues"
  },
  "homepage": "https://github.com/jneander/sandbox#readme",
  "devDependencies": {
    "@jneander/dev-tools": "^0.0.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.7.1",
    "instructure-icons": "^3.0.0",
    "instructure-ui": "^2.5.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.4.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.8",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "history": "^4.6.3",
    "normalize.css": "^7.0.0",
    "qs": "^6.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-measure": "^1.4.7",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "url-pattern": "^1.0.3"
  }
}
